{
	"extends": [
		"airbnb-base",
		"plugin:node/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"plugins": [
		"prettier"
	],
	"rules": {
		"no-console": "warn",
		"no-underscore-dangle-console": "off",
		"no-underscore-dangle": "off",
		"no-process-exit": "off",
		"class-methods-use-this": "off"
	}
}
